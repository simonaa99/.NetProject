@using Domain
@model List<Tim>

<p><a href="@Url.ActionLink("Create", "Tim")"/>Kreiraj novi tim</a></p>
<table class="table">
    <thead>
        <tr>
            <th>Naziv tima</th>
            <th>Fakultet</th>
            <th>Ažuriraj</th>
            <th>Obriši</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Tim item in Model)
        {
    <tr>
        <td>@item.NazivTima</td>
        <td>@item.Fakultet</td>
        <td><a href="@Url.ActionLink("Edit", "Tim")/@item.TimId">Ažuriraj</a></td>
        <td><a href="@Url.ActionLink("Delete", "Tim")/@item.TimId">Obrisi</a></td>
    </tr>
}


    </tbody>

</table>
